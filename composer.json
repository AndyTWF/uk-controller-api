{
    "name": "vatsim-uk/uk-controller-api",
    "description": "The web api backend to the UK Controller Plugin.",
    "keywords": ["vatsim", "uk", "ukcp", "uk-controller-api", "controller", "vatuk", "plugin"],
    "license": "GPL 3.0",
    "type": "project",
    "require": {
        "php": "^7.4",
        "ext-json": "*",
        "ext-curl": "*",
        "ext-pdo": "*",
        "ext-simplexml": "*",
        "guzzlehttp/guzzle": "^6.3",
        "bugsnag/bugsnag-laravel": "^2.17",
        "predis/predis": "~1.0",
        "laravel/passport": "^10.0",
        "laravel/framework": "^8.0",
        "beyondcode/laravel-websockets": "^2.0",
        "knplabs/github-api": "^2.12",
        "php-http/guzzle6-adapter": "^1.1",
        "nesbot/carbon": "^2.32",
        "maatwebsite/excel": "^3.1",
        "mjaschen/phpgeo": "^3.0"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "phpunit/phpunit": "^9.0",
        "mockery/mockery": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/",
            "database/seeds",
            "database/migrations"
        ],
        "psr-4": {
            "App\\": ["app/" ,"tests/app/"],
            "util\\": ["tests/util"]
        }

    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
