{
  "name": "uk-controller-api",
  "version": "3.5.0",
  "description": "Central API for the UK Controller Plugin",
  "main": "tailwind.config.js",
  "directories": {
    "doc": "docs",
    "test": "tests"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.2.1",
    "alpinejs": "^2.8.0",
    "axios": "^0.21",
